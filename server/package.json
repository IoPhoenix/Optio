{
  "name": "optio-api-endpoints",
  "version": "1.0.0",
  "engines": {
    "node": ">=10.16.0"
  },
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "babel ./src -d dist",
    "start": "NODE_ENV=production npm run build && node src/bin/www.js",
    "dev": "NODE_ENV=dev nodemon src/bin/www.js  --exec babel-node bin/www.js",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --require @babel/register --exit --timeout 30000",
    "start-mongodb-on-mac": "mongod --config /usr/local/etc/mongod.conf",
    "combo-start": "concurrently \\\"npm run --prefix ../client start\\\" \\\"nodemon --exec babel-node src/bin/www\\\" ",
    "seed-database": "node -r @babel/register 'src/helpers/seeddb.js' "
  },
  "dependencies": {
    "@material/select": "^3.1.0",
    "aws-sdk": "^2.524.0",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "connect-busboy": "0.0.2",
    "cookie-parser": "~1.4.4",
    "core-util-is": "^1.0.2",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "express": "~4.16.1",
    "express-fileupload": "^1.1.5",
    "helmet": "^3.21.1",
    "http-errors": "~1.6.3",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "mongo-seeding-cli": "^3.3.0",
    "mongoose": "^5.6.10",
    "mongoose-type-url": "^1.0.6",
    "morgan": "~1.9.1",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "uuid": "^3.3.3",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.2.0"
  }
}
